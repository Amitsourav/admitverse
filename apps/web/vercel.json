{
  "buildCommand": "npx prisma generate && next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install && npx prisma generate",
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-1.0.x"
  },
  "functions": {
    "app/**": {
      "includeFiles": "node_modules/.prisma/**"
    }
  }
}